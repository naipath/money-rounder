{
  "manifest_version": 2,
  "name": "MoneyRounder",
  "version": "1.0",
  "description": "Rounds prices to the actual prices",
  "icons": {
    "48": "icons/moneyrounder.svg",
    "96": "icons/moneyrounder.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*",
        "*://*.bol.com/*",
        "*://*.coolblue.nl/*",
        "*://*.mediamarkt.nl/*",
        "*://*.logitech.com/*",
        "*://*.tweakers.net/*",
        "*://*.ah.nl/*"
      ],
      "js": [
        "dist/main.js"
      ]
    }
  ]
}

